const fs=require("fs");
const f=require("./fs4");
const gk=(path)=>{
  return new Promise((resolve,reject)=>{
    fs.rmdir(path,err=>{
      if(err)
        reject("cannot remove directory because : File does not exists");
      else
        resolve("File exists : Directory removed");
    });
  });
};
module.exports=gk;
