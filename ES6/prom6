var ar1=[1,2,3,4];
var ar2=[0];
const arrpush=(arr1,arr2)=>{
  return new Promise((resolve, reject) => {
    for(var i=0;i<arr1.length;i++)
      arr2.push(arr1[i]);
    if((arr2.length>1))
      resolve(arr2);
    else
      reject("Error!");
  });
};

arrpush(ar1,ar2).then(result=>{
  return result;
}).catch(er=>{
  return er;
});

module.exports = {
  ar1: ar1,
  ar2: ar2,
  arrpush: arrpush
};
