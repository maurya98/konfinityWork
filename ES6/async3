function func1(s2){
  return new Promise(resolve => {
    setTimeout(() => { resolve("done!");
    }, 5000);
  });
}

async function func2(s2){
  return s2;
}

var s='';
function concatenated(s1,s2){
  s=s1+s2;
  return new Promise(resolve => {
    setTimeout(() => { resolve(s);
    }, 5000);
  });
}

module.exports = {
  concatenated: concatenated
};
