const gk=(ad,n,s)=>{
  var stk=[];
  var visited=new Array(n);
  for(var i=0;i<n;i++)
    visited[i]=0;
  var arr=[];
  stk.push(s);
  visited[s]=1;
  while(stk.length){
    i=stk.pop();
    arr.push(i);
    for(var j=n-1;j>=0;j--){
      if(visited[j]===0 && ad[i][j]===1){
        visited[j]=1;
        stk.push(j);
      }
    }
  }
  return arr;
}
module.exports=gk;
